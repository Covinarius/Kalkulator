<Window x:Class="GeneralClass.KalkulatorWPF"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeneralClass"
        mc:Ignorable="d"
        Title="Kalkulator" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="FirstLabel" HorizontalAlignment="Left" Margin="20,22,0,0" TextWrapping="Wrap" Text="Liczba 1:" VerticalAlignment="Top" FontSize="20"/>
        <TextBlock x:Name="SecondLabel" HorizontalAlignment="Left" Margin="20,81,0,0" TextWrapping="Wrap" Text="Liczba 2:" VerticalAlignment="Top" FontSize="20"/>
        <TextBox x:Name="FirstNumber" HorizontalAlignment="Left" Height="27" Margin="100,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="164" FontSize="22"/>
        <TextBox x:Name="SecondNumber" HorizontalAlignment="Left" Height="27" Margin="100,81,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="164" FontSize="20"/>

    </Grid>
</Window>
